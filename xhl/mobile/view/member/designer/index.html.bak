  <!--引入header-->
 <{include file="mobile:member/block/header.html"}>
<div data-role="page" id="pageUserFactory" class="pageUserFactory" data-dom-cache="false" data-title="设计中心">
 <div data-role="header" data-position="fixed" class="header">
     <a href="#" data-direction="reverse" data-rel="back" class="ui-btn iconfont icon-jiantou"></a>
     <h1>设计中心</h1>
     <a href="#" class="ui-btn iconfont icon-xiaoxi"></a>
 </div>
 <div data-role="main">
    <!-- 设计头 -->
     <div class="factoryU-banner">
         <a href="#">
             <img src="<{$pager.img}>/<{$MEMBER.face}>"/>
         </a>
         <h4><{$MEMBER.uname}></h4>
         <small>账户类型：<{$MEMBER.group_name}></small>
     </div>
    <!-- 设计菜单 -->
    <{foreach $menu_list as $menu}>
        <{if $menu.menu}>
            <div class="factory-list-h"><span class="iconfont icon-menu1"></span><{$menu.title}></div>
             <ul data-role="listview" class="user-listView factory-list">
                <{foreach $menu.items as $item}>
                <{if $item.menu}>
                <li data-icon="false"><a data-transition="slide" href="<{link ctl=$item.ctl}>"><span class="iconfont icon-yuandian1 " <{if $request.ctlmap.ctl==$item.ctl || $request.ctlmap.nav==$item.ctl}> on<{/if}>></span><{$item.title}><i class="iconfont icon-keyboardarrowright"></i></a> </li>
                <{/if}>
                <{/foreach}>
             </ul>
        <{/if}>
    <{/foreach}>
    
 </div>
<!--引入footer-->
<{include file="mobile:member/block/footer.html"}>